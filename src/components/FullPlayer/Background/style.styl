.wrapper {
  position: absolute;
  height: 100vh;
  width: 100vw;
  z-index: -1;
  top: 60px;
  overflow: hidden;
}

.overlay {
  background: linear-gradient(to top, rgba(60, 102, 143, 0.9), rgba(148, 184, 207, 0.9));
  position: fixed;
  height: 100vh;
  width: 100vw;
  z-index: -1;
  top: 60px;
  left: 0;
}

.backgroundImage {
  background: url('./images/default.jpg');
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 60px;
  object-fit: cover;
  filter: blur(5px);
  z-index: -2;
}
