.overlay {
  background: linear-gradient(to top, rgba(60, 102, 143, 0.9), rgba(148, 184, 207, 0.9));
  position: fixed;
  height: 100vh;
  width: 100vw;
  z-index: 4;
}

.backgroundImage {
  background: url('./images/egorka.jpg');
  position: fixed;
  height: 100vh;
  width: 100vw;
  object-fit: cover;
  margin-left: 5px;
  filter: blur(5px);
      -webkit-filter: blur(5px);
      -moz-filter: blur(5px);
      -o-filter: blur(5px);
      -ms-filter: blur(5px);
  z-index: 3;
}
